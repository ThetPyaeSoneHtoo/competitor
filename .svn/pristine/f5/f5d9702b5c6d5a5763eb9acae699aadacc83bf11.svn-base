<?xml version="1.0" encoding="UTF-8"?>
<ui:composition xmlns="http://www.w3.org/1999/xhtml" xmlns:h="http://java.sun.com/jsf/html"
                xmlns:f="http://java.sun.com/jsf/core" xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:p="http://primefaces.org/ui"
                xmlns:adm="http://github.com/adminfaces"
                template="#{layoutMB.template}">


    <ui:define name="head-end">
        <h:outputStylesheet library="css" name="competitor_datatable_style.css"/> 
        <style type="text/css">

            .ui-datatable-header{
                margin-bottom: -7px!important;
            }
        </style>
    </ui:define>

    <ui:define name="title">
        SOUTH REGION
    </ui:define>

    <ui:define name="description">

    </ui:define>
    <ui:define name="body">
        <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
        <adm:breadcrumb title="South Region" link="/view/map/southmappage.xhtml"/>
        <div class="box box-primary" style="border: white">  
            <div class="box-header with-border">
                <h:form id="mapform">
                    <div class="ui-g">
                        <div class="ui-g-12">
                            <p:commandButton value="Back" onclick="window.history.back()" styleClass="btn btn-info fa fa-reply"/>
                        </div>

                        
                            
                        
                        <div class="ui-g-2 ui-md-4 ui-lg-2 ui-fluid">
                            <h:outputLabel value="Company"/>
                            <p:selectCheckboxMenu id="multiple" value="#{mapMB.company}" label="Cities" multiple="true"
                                                  filter="false">
                                <p:ajax event="change" listener="#{mapMB.companySelect}" update="mapform:panuData,mapform:islandData,mapform:allianceData,mapform:ytlData,mapform:samwohData,mapform:simixData"/>
                                <f:selectItems value="#{mapMB.companyList}" />
                            </p:selectCheckboxMenu>
                        </div>
                    </div>

                    <h:panelGroup id="panuData">
                        <p:dataTable reflow="true" widgetVar="panuData" value="#{mapMB.mapDataList}" var="map" rendered="#{mapMB.panu_render}">
                            <f:facet name="header">
                                <h:outputLabel value="PAN UNITED" style="font-size: small;font-weight: bold;margin-left: -7px;"/>
                            </f:facet>
                            <p:column headerText="Batching Plant Location">
                                <h:outputLabel value="#{map.batchingPlantLocation}"/>
                            </p:column>
                            <p:column headerText="No. of Batching Plant">
                                <h:outputLabel value="#{map.noOfBatchingPlant}"/>
                            </p:column>
                            <p:column headerText="No. of Truck">
                                <h:outputLabel value="#{map.noOfTrack}"/>
                            </p:column>
                            <p:column headerText="Batching Capacity">
                                <h:outputLabel value="#{map.batchingCapacity}"/>
                            </p:column>
                            <p:column headerText="Map Location">
                                <p:commandButton styleClass="btn-success datatablebutton" style=" display: block; margin: 0 auto;" icon="fa fa-map-marker" onclick="PF('dlg').show();"/>
                            </p:column>
                        </p:dataTable>
                    </h:panelGroup>

                    <h:panelGroup id="islandData">
                        <p:dataTable reflow="true" widgetVar="islandData" value="#{mapMB.mapDataList}" var="map" rendered="#{mapMB.island_render}">
                            <f:facet name="header">
                                <h:outputLabel value="ISLAND" style="font-size: small;font-weight: bold;margin-left: -7px;"/>
                            </f:facet>
                            <p:column headerText="Batching Plant Location">
                                <h:outputLabel value="#{map.batchingPlantLocation}"/>
                            </p:column>
                            <p:column headerText="No. of Batching Plant">
                                <h:outputLabel value="#{map.noOfBatchingPlant}"/>
                            </p:column>
                            <p:column headerText="No. of Truck">
                                <h:outputLabel value="#{map.noOfTrack}"/>
                            </p:column>
                            <p:column headerText="Batching Capacity">
                                <h:outputLabel value="#{map.batchingCapacity}"/>
                            </p:column>
                            <p:column headerText="Map Location">
                                <p:commandButton styleClass="btn-success datatablebutton" style=" display: block; margin: 0 auto;" icon="fa fa-map-marker" onclick="PF('dlg').show();"/>
                            </p:column>
                        </p:dataTable>
                    </h:panelGroup>

                    <h:panelGroup id="allianceData">
                        <p:dataTable reflow="true" widgetVar="allianceData" value="#{mapMB.mapDataList}" var="map" rendered="#{mapMB.alliance_render}">
                            <f:facet name="header">
                                <h:outputLabel value="ALLIANCE" style="font-size: small;font-weight: bold;margin-left: -7px;"/>
                            </f:facet>
                            <p:column headerText="Batching Plant Location">
                                <h:outputLabel value="#{map.batchingPlantLocation}"/>
                            </p:column>
                            <p:column headerText="No. of Batching Plant">
                                <h:outputLabel value="#{map.noOfBatchingPlant}"/>
                            </p:column>
                            <p:column headerText="No. of Truck">
                                <h:outputLabel value="#{map.noOfTrack}"/>
                            </p:column>
                            <p:column headerText="Batching Capacity">
                                <h:outputLabel value="#{map.batchingCapacity}"/>
                            </p:column>
                            <p:column headerText="Map Location">
                                <p:commandButton styleClass="btn-success datatablebutton" style=" display: block; margin: 0 auto;" icon="fa fa-map-marker" onclick="PF('dlg').show();"/>
                            </p:column>
                        </p:dataTable>
                    </h:panelGroup>



                    <h:panelGroup id="ytlData">
                        <p:dataTable reflow="true" widgetVar="allianceData" value="#{mapMB.mapDataList}" var="map" rendered="#{mapMB.ytl_render}">
                            <f:facet name="header">
                                <h:outputLabel value="YTL" style="font-size: small;font-weight: bold;margin-left: -7px;"/>
                            </f:facet>
                            <p:column headerText="Batching Plant Location">
                                <h:outputLabel value="#{map.batchingPlantLocation}"/>
                            </p:column>
                            <p:column headerText="No. of Batching Plant">
                                <h:outputLabel value="#{map.noOfBatchingPlant}"/>
                            </p:column>
                            <p:column headerText="No. of Truck">
                                <h:outputLabel value="#{map.noOfTrack}"/>
                            </p:column>
                            <p:column headerText="Batching Capacity">
                                <h:outputLabel value="#{map.batchingCapacity}"/>
                            </p:column>
                            <p:column headerText="Map Location">
                                <p:commandButton styleClass="btn-success datatablebutton" style=" display: block; margin: 0 auto;" icon="fa fa-map-marker" onclick="PF('dlg').show();"/>
                            </p:column>
                        </p:dataTable>
                    </h:panelGroup>

                    <h:panelGroup id="samwohData">
                        <p:dataTable reflow="true" widgetVar="allianceData" value="#{mapMB.mapDataList}" var="map" rendered="#{mapMB.samwoh_render}">
                            <f:facet name="header">
                                <h:outputLabel value="SAMWOH" style="font-size: small;font-weight: bold;margin-left: -7px;"/>
                            </f:facet>
                            <p:column headerText="Batching Plant Location">
                                <h:outputLabel value="#{map.batchingPlantLocation}"/>
                            </p:column>
                            <p:column headerText="No. of Batching Plant">
                                <h:outputLabel value="#{map.noOfBatchingPlant}"/>
                            </p:column>
                            <p:column headerText="No. of Truck">
                                <h:outputLabel value="#{map.noOfTrack}"/>
                            </p:column>
                            <p:column headerText="Batching Capacity">
                                <h:outputLabel value="#{map.batchingCapacity}"/>
                            </p:column>
                            <p:column headerText="Map Location">
                                <p:commandButton styleClass="btn-success datatablebutton" style=" display: block; margin: 0 auto;" icon="fa fa-map-marker" onclick="PF('dlg').show();"/>
                            </p:column>
                        </p:dataTable>
                    </h:panelGroup>

                    <h:panelGroup id="simixData">
                        <p:dataTable reflow="true" widgetVar="allianceData" value="#{mapMB.mapDataList}" var="map" rendered="#{mapMB.simix_render}">
                            <f:facet name="header">
                                <h:outputLabel value="SIMIX" style="font-size: small;font-weight: bold;margin-left: -7px;"/>
                            </f:facet>
                            <p:column headerText="Batching Plant Location">
                                <h:outputLabel value="#{map.batchingPlantLocation}"/>
                            </p:column>
                            <p:column headerText="No. of Batching Plant">
                                <h:outputLabel value="#{map.noOfBatchingPlant}"/>
                            </p:column>
                            <p:column headerText="No. of Truck">
                                <h:outputLabel value="#{map.noOfTrack}"/>
                            </p:column>
                            <p:column headerText="Batching Capacity">
                                <h:outputLabel value="#{map.batchingCapacity}"/>
                            </p:column>
                            <p:column headerText="Map Location">
                                <p:commandButton styleClass="btn-success datatablebutton" style=" display: block; margin: 0 auto;" icon="fa fa-map-marker" onclick="PF('dlg').show();"/>
                            </p:column>
                        </p:dataTable>
                    </h:panelGroup>
                </h:form>
            </div>
        </div>
        <p:dialog id="gmapDialog" widgetVar="dlg" width="625" height="400" modal="true" appendTo="@(body)" positionType="absolute">
            <p:gmap id="gmap" center="1.418,103.751004" zoom="15" type="HYBRID" style="width:100%;height:400px" />
        </p:dialog>
    </ui:define>
</ui:composition>
